{
  "port": 8080,
  "services": {
    "testService": {
      "name": "testService",
      "hosts": [
        {
          "host": "localhost",
          "port": 1234
        }
      ],
      "protocol": "http",
      "heartbeat": "/api/heartbeat",
      "routes": [
        {
          "path": "/test",
          "method": "GET",
          "gatewayPath": "/test",
          "authenticated": false
        },
        {
          "path": "/test",
          "method": "GET",
          "gatewayPath": "/testauth",
          "authenticated": true
        }
      ]
    },
    "userService": {
      "name": "userService",
      "hosts": [
        {
          "host": "localhost",
          "port": 3000
        }
      ],
      "protocol": "http",
      "heartbeat": "/api/heartbeat",
      "routes": [
        {
          "path": "/api/signup",
          "gatewayPath": "/api/signup",
          "method": "POST",
          "authenticated": false
        },
        {
          "path": "/api/updateAccount",
          "gatewayPath": "/api/updateAccount",
          "method": "PUT",
          "authenticated": true
        },
        {
          "path": "/api/isUserHome",
          "gatewayPath": "/api/isUserHome",
          "method": "GET",
          "authenticated": true
        },
        {
          "path": "/api/getUserInfo",
          "gatewayPath": "/api/getUserInfo",
          "method": "GET",
          "authenticated": true
        }
      ]
    },
    "partyQueryService": {
      "name": "partyQueryService",
      "hosts": [
        {
          "host": "localhost",
          "port": 4000
        }
      ],
      "protocol": "http",
      "heartbeat": "/api/heartbeat",
      "routes": [
        {
          "path": "/api/partyInvite",
          "gatewayPath": "/api/partyInvite",
          "method": "GET",
          "authenticated": true
        },
        {
          "path": "/api/parties",
          "gatewayPath": "/api/parties",
          "method": "GET",
          "authenticated": true
        },
        {
          "path": "/api/partyStatus",
          "gatewayPath": "/api/partyStatus",
          "method": "GET",
          "authenticated": true
        }
      ]
    }
  }
}
