{
  "port": 8080,
  "services": {
    "testService": {
      "name": "testService",
      "hosts": ["test-service"],
      "port": 1234,
      "protocol": "http",
      "routes": [
        {
          "path": "/test",
          "method": "GET",
          "gatewayPath": "/test",
          "authenticated": false
        },
        {
          "path": "/test",
          "method": "GET",
          "gatewayPath": "/testauth",
          "authenticated": true
        }
      ]
    },
    "userService": {
      "name": "userService",
      "hosts": ["user-service"],
      "port": 3000,
      "protocol": "http",
      "routes": [
        {
          "path": "/api/signup",
          "gatewayPath": "/api/signup",
          "method": "POST",
          "authenticated": false
        },
        {
          "path": "/api/updateAccount",
          "gatewayPath": "/api/updateAccount",
          "method": "PUT",
          "authenticated": true
        },
        {
          "path": "/api/isUserHome",
          "gatewayPath": "/api/isUserHome",
          "method": "GET",
          "authenticated": true
        },
        {
          "path": "/api/getUserInfo",
          "gatewayPath": "/api/getUserInfo",
          "method": "GET",
          "authenticated": false
        }
      ]
    },
    "partyCommandService": {
      "name": "partyCommandService",
      "hosts": ["party-command-service"],
      "port": 5000,
      "protocol": "http",
      "routes": [
        {
          "path": "/api/create-party",
          "gatewayPath": "/api/create-party",
          "method": "POST",
          "authenticated": true
        },
        {
          "path": "/api/join-party",
          "gatewayPath": "/api/join-party",
          "method": "POST",
          "authenticated": true
        }
      ]
    }
  }
}
